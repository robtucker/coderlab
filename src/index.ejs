<!DOCTYPE html>
<html>
<head>

    <base href="/">
    
    <!-- metas -->
    <% _.each(htmlWebpackPlugin.options.metas, function(meta) { %>
    <meta <% Object.keys(meta).forEach(function(attr) {%>${ attr }="${ meta[attr] }" <% }); %>/>
    <% }); %>
    
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <title><%= htmlWebpackPlugin.options.title %></title>

    <% _.each(htmlWebpackPlugin.options.cdns, function(cdn) { %>
    <script src=${ cdn.src }></script>
    <% }); %>
    
</head>
<body class="marginless">

    <div id="app">Loading...</div>

</body>
</html>
